<table class="table">
  <thead>
    <tr>
      <th scope="col">Toy Name</th>
      <th scope="col">Content</th>
      <th scope="col">Price</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody class="table-group-divider">
    <% @posts.each do |post| %>
    <tr>
            <td> <%= post.toyName%> </td>
            <td> <%= post.content%> </td>
            <td> <%= post.price%> </td>
            <td> <%= post.email%> </td>
    </tr>
     <% end %>
    
    

  </tbody>
</table>
